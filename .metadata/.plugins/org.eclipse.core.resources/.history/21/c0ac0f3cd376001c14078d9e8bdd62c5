package com.controlstatement.pro;

public class Variable {
	static int a=52;
	private static int amount(int principle , int price ) {
		return principle+price;
		
	}
	static {
		System.out.println("my name");
	}
	static {
		System.out.println("kanchana");
	}
	public static void main(String[] args) {
		int a=5;
		int b=2;
		int c=a+b;
		System.out.println(c);
		
	}

}
